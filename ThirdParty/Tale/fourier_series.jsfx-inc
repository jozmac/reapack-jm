// High-pass filtered sawtooth

function four_hpsaw()
  instance(size, fc)
  local(cos, sin, k)
(
  cos = this.four_setdc(fc > 0 ? -2/(exp(-2*$pi * fc) - 1) - 1/$pi / fc - 1 : 0, 0);
  sin = cos + 1;

  k = 1;
  loop(size / 2 - 1,
    cos[] = -2/$pi * fc / (sqr(fc) + sqr(k));
    sin[] = 2/$pi * k / (sqr(fc) + sqr(k));

    // Low-pass
    // cos[] = 2/$pi * fc / (sqr(fc) + sqr(k));
    // sin[] = 2/$pi * fc*fc / ((sqr(fc) + sqr(k)) * k);

    k += 1; cos += 2; sin += 2;
  );
);

