// High-pass filtered square

function four_hpsqr()
  instance(size, fc)
  local(cos, sin, k)
(
  // Non-normalized

  cos = this.four_setdc(0, 0);
  sin = cos + 1;

  k = 1;
  loop(size / 4,
    cos[] = -4/$pi * fc / (sqr(fc) + sqr(k));
    sin[] = 4/$pi * k / (sqr(fc) + sqr(k));

    k += 2; cos[2] = sin[2] = 0; cos += 4; sin += 4;
  );
);

